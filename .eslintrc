{
  "extends": "airbnb",
  "rules": {
    "strict": 0,
    "no-unused-vars": 1,
    "class-methods-use-this": 0,
    "no-console": 0,
    "consistent-return": 0,
    "no-underscore-dangle": 0,
    "no-plusplus": 0,
    "no-param-reassign": 0,
    "no-restricted-syntax": 0,
    "global-require": 1,
    "comma-dangle": 0
  }
}